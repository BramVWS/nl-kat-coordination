[
  {
    "name": "BinaryEdge",
    "module": "kat_binaryedge.scan",
    "description": "Use BinaryEdge to find open ports with vulnerabilities that are found on that port",
    "consumes": ["IPAddressV4", "IPAddressV6"],
    "produces": [
      "IPService",
      "IPPort",
      "KATFindingType",
      "SoftwareInstance",
      "Software",
      "Finding",
      "Service",
      "CVEFindingType"
